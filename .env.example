# Discord Bot Configuration
DISCORD_TOKEN=your_discord_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here
# Optional: for dev-only guild-scoped slash commands (instant deploy)
# DISCORD_GUILD_ID=your_test_guild_id

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
# No longer need GEMINI_MODEL - bot automatically uses multiple models with fallback

# Sogni AI Configuration
SOGNI_USERNAME=your_sogni_username
SOGNI_PASSWORD=your_sogni_password
# (Optional) specific model IDs. If left empty, the bot picks the best available.
IMAGE_MODEL_ID=flux1-schnell-fp8
VIDEO_MODEL_ID=wan_v2.2-14b-fp8_t2v_lightx2v
APP_ID=BlockAerie Sketch
JSON_RPC_URL=https://base.publicnode.com

# Bot Settings
COMMAND_PREFIX=!
SESSION_TIMEOUT_MS=300000
DM_POLICY=open
ADMIN_USER_ID=your_discord_user_id

# ── New settings (all optional with defaults) ──────────────────

# Sogni Gen CLI path (defaults to ~/.openclaw/workspace/skills/sogni-gen/sogni-gen.mjs)
# SOGNI_GEN_PATH=/absolute/path/to/sogni-gen.mjs

# Default models for slash commands
# SOGNI_DEFAULT_IMAGE_MODEL=z_image_turbo_bf16
# SOGNI_DEFAULT_EDIT_MODEL=qwen_image_edit_2511_fp8_lightning

# Default dimensions
# SOGNI_DEFAULT_WIDTH=768
# SOGNI_DEFAULT_HEIGHT=768

# Seed strategy: 'prompt-hash' (deterministic) or 'random'
# SOGNI_SEED_STRATEGY=prompt-hash

# Memory database path (defaults to ./data/memory.sqlite)
# MEMORY_DB_PATH=./data/memory.sqlite
# Short-term memory TTL in ms (default: 30 min)
# MEMORY_SHORT_TERM_TTL_MS=1800000

# Scheduler database path (defaults to ./data/reminders.sqlite)
# SCHEDULER_DB_PATH=./data/reminders.sqlite
# Scheduler poll interval in ms (default: 15s)
# SCHEDULER_POLL_MS=15000

# Per-user cooldown between slash commands in ms (default: 3s)
# USER_COOLDOWN_MS=3000